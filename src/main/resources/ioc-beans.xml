<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="kitten" class="io.github.touhidion.ioc.Kitten">
        <property name="kittenName" value="Tom kid"/>
    </bean>
    <bean id="dogKid" class="io.github.touhidion.ioc.DogKid">
        <constructor-arg name="dogKidName" value="Dog kid"/>
    </bean>
   <bean id="animal" class="io.github.touhidion.ioc.Cat">
       <constructor-arg name="name" value="Tom"></constructor-arg>
       <property name="kitten" ref="kitten"></property>
       <property name="dogKid" ref="dogKid"></property>
   </bean>

</beans>